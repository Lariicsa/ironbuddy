<h1 class="title is-2 has-text-centered title-home">Welcome Ironadmin</h1>
<div class="columns is-multiline is-mobile is-centered">
	<div class="column is-one-third">
		<div class="box home">
			<span class="title is-5 margin-tb">My profile</span>
			<div class="picture x4 margin-tb" style="background-image: url('{{user.img}}')"></div>
			<div class="columns is-mobile is-centered">
				<div class="column">
					<p class="subtitle is-6 name">{{user.name}} {{user.lastname}}</p>
					<a class="button is-blue is-fullwidth" href="/profile">View info</a>
				</div>
			</div>
		</div>
	</div>
	<div class="column is-one-third">
		<div class="box home">
			<div class="columns is-mobile is-centered">
				<div class="column">
					<p class="title is-5">Add User</p>
					<a href="/auth/add-user" class="icon resource center">
						<i class="fas fa-plus-circle"></i>
					</a>
				</div>
			</div>
		</div>
	</div>
</div>